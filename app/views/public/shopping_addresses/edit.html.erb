<h4>配送先編集</h4>
<%= form_with model: @shopping_address, local: true do |f| %>
  <div>
    <%= f.label :郵便番号（ハイフンなし） %>
    <%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code", placeholder:"0000000" %>
  </div>

  <div>
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :宛名 %>
    <%= f.text_field :name %>
  </div>

  <div>
  <%= f.submit :変更を保存, class: 'btn btn-success' %>
  </div>
<% end %>